<h1>Home Page</h1>

<h2>Users</h2>

    <p>Username</p>
    <p>Email</p>
    <p>Age</p>


  <% @users.each do |entry| %>
  
    <p><%= entry.username %></p>
    <p><%= entry.email %></p>
    <p><%= entry.age.to_s %></p>
 
  <% end %>


<h2>Cats</h2>


<p>Name</p>

  <% @cats.each do |entry| %>
  <%= entry.name %>
  <% end %>

<h2>Todos</h2>

  <%- <% @todo.each do |entry| %>
 <%- <%= todo.name %>
  <%-<% end %>